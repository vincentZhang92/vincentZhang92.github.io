{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "strictFunctionTypes": false,
    "jsx": "preserve",
    "baseUrl": ".",
    "allowJs": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "experimentalDecorators": true,
    "lib": ["dom", "esnext"],
    "noImplicitAny": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "noImplicitThis": true,
    "importHelpers": true,
    "removeComments": true,
    "types": ["vite/client"],
    "paths": {
      "@theme/*": ["docs/.vitepress/theme/*"],
    },
    "typeRoots": [
      "node_modules/@types/",
      "types/"
    ]
  },
  "include": [
    "docs/.vitepress/theme/**/*.ts",
    "docs/.vitepress/theme/**/*.d.ts",
    "docs/.vitepress/theme/**/*.tsx",
    "docs/.vitepress/theme/**/*.vue",
    "docs/.vitepress/**/*.ts",
    "docs/.vitepress/**/*.d.ts",
    "types/**/*.d.ts"
  ],
  "exclude": ["node_modules", "dist", "**/*.js"]
}